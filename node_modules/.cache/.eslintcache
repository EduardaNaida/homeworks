5/HW5\";\n\nfunction App() {\n    return (\n        <div className={s.App}>\n      {/*      <HW1/>\n            раскомментировать по ходу выполнения\n            <HW2/>\n            <HW3/>\n            <HW4/>*/}\n\n            {/*при выполнении дз 5 и более - закомментировать здесь дз 1-4, так как они есть внутри дз 5*/}\n            <HW5/>\n        </div>\n    )\n}\n\nexport default App\n","/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw10/bll/store.ts",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/HW5.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw10/bll/loadingReducer.ts",[],[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw12/bll/themeReducer.ts",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/Pages.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/layout/Layout.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/pages/PreJunior.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/pages/Error404.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/pages/Junior.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/pages/JuniorPlus.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/header/Header.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw05/sidebar/Sidebar.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw02/HW2.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw01/HW1.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw03/HW3.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw04/HW4.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw03/GreetingContainer.tsx",["250"],[],"import React, { ChangeEvent, KeyboardEvent, useState } from 'react'\nimport Greeting from './Greeting'\nimport { UserType } from './HW3'\nimport user from \"../hw08/User\";\n\ntype GreetingContainerPropsType = {\n    users: Array<UserType> // need to fix any\n    addUserCallback: (name: string) => void // need to fix any\n}\n\nexport const pureAddUser = (name: any, setError: any, setName: any, addUserCallback: any) => {\n    // если имя пустое - показать ошибку, иначе - добавить юзера и очистить инпут\n    if (name.trim().length === 0) {\n        setError('Please enter name!')\n    } else {\n        addUserCallback(name)\n        setName(name)\n        //setError('')\n        alert(`Hello ${name}!`)\n    }\n}\n\nexport const pureOnBlur = (name: any, setError: any) => { // если имя пустое - показать ошибку\n    if (name.trim().length === 0) {\n        setError('Please enter name!')\n    }\n}\n\nexport const pureOnEnter = (e: KeyboardEvent<HTMLInputElement>, addUser: any) => { // если нажата кнопка Enter - добавить\n    if(e.key === 'Enter'){\n        addUser();\n    }\n}\n\n// более простой и понятный для новичков\n// function GreetingContainer(props: GreetingPropsType) {\n\n// более современный и удобный для про :)\nconst GreetingContainer: React.FC<GreetingContainerPropsType> = ({\n    users,\n    addUserCallback,\n}) => {\n    // деструктуризация пропсов\n    const [name, setName] = useState<string>('') // need to fix any\n    const [error, setError] = useState<string>('') // need to fix any\n\n    const setNameCallback = (e: ChangeEvent<HTMLInputElement>) => { // need to fix any\n        setName(e.currentTarget.value) // need to fix\n\n       error && setError('')\n    }\n    const addUser = () => {\n        pureAddUser(name, setError, setName, addUserCallback)\n        setName('')\n    }\n\n    const onBlur = () => {\n        pureOnBlur(name, setError)\n    }\n\n    const onEnter = (e: any) => {\n        pureOnEnter(e, addUser)\n    }\n\n    const totalUsers = users.length // need to fix\n    const lastUserName = users.length > 0 ? users[totalUsers-1].name : \"\"//Users[totalUsers-1] // need to fix\n\n    return (\n        <Greeting\n            name={name}\n            setNameCallback={setNameCallback}\n            addUser={addUser}\n            onBlur={onBlur}\n            onEnter={onEnter}\n            error={error}\n            totalUsers={totalUsers}\n            lastUserName={lastUserName}\n        />\n    )\n}\n\nexport default GreetingContainer\n","/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw04/Stand.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw02/affairs/Affairs.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw01/message/Message.tsx",["251","252"],[],"import React from 'react'\nimport s from './Message.module.css'\nimport {MessageType} from \"../HW1\";\nimport {UserType} from \"../../hw03/HW3\";\n\n// нужно создать правильный тип вместо any\nexport type MessagePropsType = {\n    message: MessageType\n}\n\n// нужно отобразить приходящие данные\nconst Message = (props: MessagePropsType) => {\n    return (\n        <div id={'hw1-message-' + props.message.id} className={s.message}>\n            <div className={s.imageAndText}>\n                <img\n                    id={'hw1-avatar-' + props.message.id}\n                    // создаёт студент\n                    src={props.message.user.avatar}\n                    //\n                />\n                <div className={s.angle}/>\n                <div className={s.text}>\n                    <div id={'hw1-name-' + props.message.id} className={s.name}>\n                        {props.message.user.name}\n                    </div>\n                    <pre id={'hw1-text-' + props.message.id} className={s.messageText}>\n                        {props.message.message.text}\n                    </pre>\n                </div>\n            </div>\n            <div id={'hw1-time-' + props.message.id} className={s.time}>\n                {props.message.message.time}\n            </div>\n        </div>\n    )\n}\n\nexport default Message\n","/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw01/message-sender/MessageSender.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw01/friend-message/FriendMessage.tsx",["253"],[],"import React from 'react'\nimport s from './FriendMessage.module.css'\nimport {MessageType} from \"../HW1\";\n\nexport type FriendPropsType = {\n    message: MessageType\n}\n// создать тип вместо any и отобразить приходящие данные\nconst FriendMessage = (props: FriendPropsType) => {\n    return (\n        <div\n            id={'hw1-friend-message-' + props.message.id}\n            className={s.friendMessage}\n        >\n            <div className={s.friendImageAndText}>\n                <img\n                    id={'hw1-friend-avatar-' + props.message.id}\n                    src={props.message.user.avatar}\n                />\n                <div className={s.angle}/>\n                <div className={s.friendText}>\n                    <div\n                        id={'hw1-friend-name-' + props.message.id}\n                        className={s.friendName}\n                    >\n                        {props.message.user.name}\n                    </div>\n                    <pre\n                        id={'hw1-friend-text-' + props.message.id}\n                        className={s.friendMessageText}\n                    >\n                        {props.message.message.text}\n                    </pre>\n                </div>\n            </div>\n            <div\n                id={'hw1-friend-time-' + props.message.id}\n                className={s.friendTime}\n            >\n                {props.message.message.time}\n            </div>\n        </div>\n    )\n}\n\nexport default FriendMessage\n","/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw03/Greeting.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw04/common/c1-SuperInputText/SuperInputText.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw02/affairs/affair/Affair.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw04/common/c2-SuperButton/SuperButton.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw04/common/c3-SuperCheckbox/SuperCheckbox.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw06/HW6.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw06/localStorage/localStorage.ts",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw06/common/c4-SuperEditableSpan/SuperEditableSpan.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw07/HW7.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw07/common/c5-SuperSelect/SuperSelect.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw07/common/c6-SuperRadio/SuperRadio.tsx",[],[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw08/HW8.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw08/User.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw08/bll/homeWorkReducer.ts",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw09/HW9.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw09/Clock.tsx",["254"],[],"import React, {useState} from 'react'\nimport SuperButton from '../hw04/common/c2-SuperButton/SuperButton'\nimport {restoreState} from '../hw06/localStorage/localStorage'\nimport s from './Clock.module.css'\n\nfunction Clock() {\n    const [timerId, setTimerId] = useState<number | undefined>(undefined)\n    // for autotests // не менять // можно подсунуть в локалСторэдж нужную дату, чтоб увидеть как она отображается\n    const [date, setDate] = useState<Date>(new Date(restoreState('hw9-date', Date.now())))\n    const [show, setShow] = useState<boolean>(false)\n\n    const start = () => {\n        // пишут студенты // запустить часы (должно отображаться реальное время, а не +1)\n        // сохранить ид таймера (https://learn.javascript.ru/settimeout-setinterval#setinterval)\n        const id: number = window.setInterval(() => {\n            setDate(new Date)\n        }, 1000)\n        setTimerId(id)\n\n    }\n\n    const stop = () => {\n        // пишут студенты // поставить часы на паузу, обнулить ид таймера (timerId <- undefined)\n        clearInterval(timerId)\n        setTimerId(undefined)\n    }\n\n    const onMouseEnter = () => { // пишут студенты // показать дату если наведена мышка\n        setShow(true)\n    }\n    const onMouseLeave = () => { // пишут студенты // спрятать дату если мышка не наведена\n        setShow(false)\n    }\n    let hours = date.getHours()\n    let minutes = date.getMinutes()\n    let seconds = date.getSeconds()\n\n    const stringTime = `${hours}:${minutes}:${seconds}` || <br/> // часы24:минуты:секунды (01:02:03)/(23:02:03)/(24:00:00)/(00:00:01) // пишут студенты\n    const stringDate = `${date.getDate()}.${date.getMonth() + 1}.${date.getFullYear()}` || <br/> // день.месяц.год (01.02.2022) // пишут студенты, варианты 01.02.0123/01.02.-123/01.02.12345 не рассматриваем\n\n    // день недели на английском, месяц на английском (https://learn.javascript.ru/intl#intl-datetimeformat)\n    const stringDay = date.toLocaleString('en-US', {weekday: 'long'}) || <br/> // пишут студенты\n    const stringMonth = date.toLocaleString('en-US', {month: 'long'}) || <br/> // пишут студенты\n\n    return (\n        <div className={s.clock}>\n            <div\n                id={'hw9-watch'}\n                className={s.watch}\n                onMouseEnter={onMouseEnter}\n                onMouseLeave={onMouseLeave}\n            >\n                <span id={'hw9-day'}>{stringDay}</span>,{' '}\n                <span id={'hw9-time'}>\n                    <strong>{stringTime}</strong>\n                </span>\n            </div>\n\n            <div id={'hw9-more'}>\n                <div className={s.more}>\n                    {show ? (\n                        <>\n                            <span id={'hw9-month'}>{stringMonth}</span>,{' '}\n                            <span id={'hw9-date'}>{stringDate}</span>\n                        </>\n                    ) : (\n                        <>\n                            <br/>\n                        </>\n                    )}\n                </div>\n            </div>\n\n            <div className={s.buttonsContainer}>\n                <SuperButton\n                    className={s.ButtonStart}\n                    id={'hw9-button-start'}\n                    disabled={timerId !== undefined}// пишут студенты // задизэйблить если таймер запущен\n                    onClick={start}\n                >\n                    Start\n                </SuperButton>\n                <SuperButton\n                    className={s.ButtonStop}\n                    id={'hw9-button-stop'}\n                    disabled={timerId === undefined}// пишут студенты // задизэйблить если таймер не запущен\n                    onClick={stop}\n                >\n                    Stop\n                </SuperButton>\n            </div>\n        </div>\n    )\n}\n\nexport default Clock\n","/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw10/HW10.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw10/Loader.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw11/HW11.tsx",["255"],[],"import React, {ChangeEvent, useState} from 'react'\nimport s from './HW11.module.css'\nimport s2 from '../../s1-main/App.module.css'\nimport {restoreState} from '../hw06/localStorage/localStorage'\nimport SuperRange from './common/c7-SuperRange/SuperRange'\n\n/*\n* 1 - передать значения в оба слайдера\n* 2 - дописать типы и логику функции change\n* 3 - сделать стили в соответствии с дизайном\n* */\n\nfunction HW11() {\n    // for autotests // не менять // можно подсунуть в локалСторэдж нужные числа, чтоб увидеть как они отображаются\n    const [value1, setValue1] = useState(restoreState<number>('hw11-value1', 0))\n    const [value2, setValue2] = useState(restoreState<number>('hw11-value2', 100))\n\n    const change = (event: Event, newValue: number | number[]) => {\n        setValue1(newValue as number)\n    }\n\n    const handleChange = (event: Event, newValue: number | number[]) => {\n        if (typeof newValue === 'object') {\n            setValue1(newValue[0])\n            setValue2(newValue[1])\n        }\n    };\n\n    return (\n        <div id={'hw11'}>\n            <div className={s2.hwTitle}>Homework #11</div>\n\n            <div className={s2.hw}>\n                <div className={s.container}>\n                    <div className={s.wrapper}>\n                        <span id={'hw11-value'} className={s.number}>{value1}</span>\n                        <SuperRange\n                            id={'hw11-single-slider'}\n                            className={s.slider}\n                            value={value1}\n                            // сделать так чтоб value1 изменялось // пишет студент\n                            onChange={change}\n\n                        />\n                    </div>\n                    <div className={s.wrapper}>\n                        <span id={'hw11-value-1'} className={s.number}>{value1}</span>\n                        <SuperRange\n                            id={'hw11-double-slider'}\n                            className={s.slider}\n                            // сделать так чтоб value1/2 изменялось // пишет студент\n                            value={[value1, value2]}\n                            valueLabelDisplay=\"auto\"\n                            // сделать так чтоб value1 изменялось // пишет студент\n                            onChange={handleChange}\n                        />\n                        <span id={'hw11-value-2'} className={s.number}>{value2}</span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default HW11\n","/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw11/common/c7-SuperRange/SuperRange.tsx",[],[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw12/HW12.tsx",[],[],"/Users/eduardanaida/Documents/GitHub/homeworks/src/s2-homeworks/hw13/HW13.tsx",[],[],{"ruleId":"256","severity":1,"message":"257","line":3,"column":8,"nodeType":"258","messageId":"259","endLine":3,"endColumn":11},{"ruleId":"256","severity":1,"message":"260","line":4,"column":8,"nodeType":"258","messageId":"259","endLine":4,"endColumn":11},{"ruleId":"256","severity":1,"message":"261","line":5,"column":8,"nodeType":"258","messageId":"259","endLine":5,"endColumn":11},{"ruleId":"256","severity":1,"message":"262","line":6,"column":8,"nodeType":"258","messageId":"259","endLine":6,"endColumn":11},{"ruleId":"256","severity":1,"message":"263","line":4,"column":8,"nodeType":"258","messageId":"259","endLine":4,"endColumn":12},{"ruleId":"256","severity":1,"message":"264","line":4,"column":9,"nodeType":"258","messageId":"259","endLine":4,"endColumn":17},{"ruleId":"265","severity":1,"message":"266","line":16,"column":17,"nodeType":"267","endLine":21,"endColumn":19},{"ruleId":"265","severity":1,"message":"266","line":16,"column":17,"nodeType":"267","endLine":19,"endColumn":19},{"ruleId":"268","severity":1,"message":"269","line":16,"column":21,"nodeType":"270","messageId":"271","endLine":16,"endColumn":29,"fix":"272"},{"ruleId":"256","severity":1,"message":"273","line":1,"column":16,"nodeType":"258","messageId":"259","endLine":1,"endColumn":27},"@typescript-eslint/no-unused-vars","'HW1' is defined but never used.","Identifier","unusedVar","'HW2' is defined but never used.","'HW3' is defined but never used.","'HW4' is d